<div class="row light-green lighten-4 z-depth-3">
  <form action="/user/{{userId}}/menu" method="POST">
    <div>
      <label for="name">What would you like to call it?</label>
      <input type="text" name="name" placeholder="Menu Name" />
    </div>
    <button type="submit">Save New Menu</button>
  </form>
</div>

<div class="return row">
  <div class="col s3 push-s1">
    <a href="/user/{{userId}}">Return</a>
  </div>
</div>